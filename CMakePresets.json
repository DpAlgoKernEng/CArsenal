{
  "version": 3,
  "configurePresets": [
    {
      "name": "vcpkg-release",
      "generator": "Unix Makefiles",
      "binaryDir": "${sourceDir}/build_cmake",
      "cacheVariables": {
        "CMAKE_TOOLCHAIN_FILE": {
          "type": "FILEPATH",
          "value": "${sourceDir}/third/lib_manager/vcpkg/scripts/buildsystems/vcpkg.cmake"
        },
        "CMAKE_BUILD_TYPE": "Release"
      }
    },
    {
      "name": "vcpkg-debug",
      "inherits": "vcpkg-release",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug"
      }
    }
  ]
}